{
	"compilerOptions": {
		"jsx": "react",
		"declaration": true,
		"strictNullChecks": true,
		"target": "es2017",
		"outDir": "dist",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"lib": ["es2015", "dom", "es2017"],
		"rootDir": "source",
		"esModuleInterop": true
	},
	"include": ["source"],
	"exclude": ["node_modules", "dist"]
}
